{
  "name": "auto-tspress",
  "version": "0.0.24",
  "description": "通过tsdoc快速生成项目中工具函数的文档",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "rollup -c",
    "r": "node ./lib/index",
    "dev": "rollup  --config rollup.dev.config.js && node ./lib/index",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "bin": {
    "auto-tspress": "./lib/index.js"
  },
  "author": "shiouhoo",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@shiouhoo/eslint-config": "^1.0.8",
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "rollup": "^3.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "element-plus": "^2.3.12",
    "ts-morph": "^19.0.0",
    "vitepress": "1.0.0-rc.9"
  },
  "files": [
    "docs/.vitepress/components",
    "docs/.vitepress/theme",
    "docs/.vitepress/config.js",
    "docs/.vitepress/first.md",
    "docs/.vitepress/index.md",
    "lib"
  ],
  "keywords": [
    "tsdoc",
    "文档生成"
  ]
}